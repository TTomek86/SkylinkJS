<!DOCTYPE html>
<html lang="en">

<head>
  <title>SkylinkJS {{projectVersion}}</title>

  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">

  <link rel="shortcut icon" type="image/ico" href="{{projectAssets}}/favicon.ico">

  <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Source+Sans+Pro:300,400,600,700|Source+Sans+Pro"
    type="text/css">
  <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Source+Sans+Pro:300,400,600,700|Source+Code+Pro"
    type="text/css">
  <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/font-awesome/4.6.1/css/font-awesome.min.css"
    type="text/css">


  <!-- CSS -->
  <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.6/css/bootstrap.min.css">
  <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.6/css/bootstrap-theme.min.css">
  <link href="{{projectAssets}}/vendor/css/font-awesome.min.css" rel="stylesheet" type="text/css">

  <link rel="stylesheet" href="{{projectAssets}}/vendor/prettify/prettify-min.css">
  <link rel="stylesheet" href="{{projectAssets}}/css/style.css">

  <!-- Javascripts -->
  <script src="https://code.jquery.com/jquery-1.12.3.min.js"></script>
  <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.6/js/bootstrap.min.js"></script>
  <script src="{{projectAssets}}/js/script.js"></script>
  <script src="{{yuiSeedUrl}}"></script>
</head>

<body>
  <div id="doc">
    <nav id="hd" class="navbar navbar-inverse navbar-fixed-top" role="navigation">
      <div class="container">
        <div class="navbar-header">
          <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#navbar" aria-expanded="false" aria-controls="navbar">
            <span class="sr-only">Toggle navigation</span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
          </button>
          <a href="{{url}}" class="navbar-brand">
            <img src="{{projectAssets}}/img/logo.svg" />JS<small>Version: {{projectVersion}}</small>
          </a>
        </div>
        <div id="navbar" class="navbar-collapse collapse">
          {{>sidebar}}
        </div>
        <!--/.navbar-collapse -->
      </div>
    </nav>
    <div id="bd" class="yui3-g">
      <div class="yui3-u-1-4">
      </div>
      <div class="yui3-u-3-4">
        {{>options}}
        <div class="apidocs">
          <div id="docs-main">
            <div class="content content-main">
              {{>layout_content}}
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
  <script src="{{projectAssets}}/vendor/prettify/prettify-min.js"></script>
  <script>
  prettyPrint();
  </script>
  <script src="{{projectAssets}}/vendor/yui/yui-prettify.js"></script>
  <script src="{{projectAssets}}/../api.js"></script>
  <script src="{{projectAssets}}/vendor/yui/api-filter.js"></script>
  <script src="{{projectAssets}}/vendor/yui/api-list.js"></script>
  <script src="{{projectAssets}}/vendor/yui/api-search.js"></script>
  <script src="{{projectAssets}}/vendor/yui/apidocs.js"></script>
</body>

</html>
